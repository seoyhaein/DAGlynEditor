<Window xmlns="https://github.com/avaloniaui"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:controls="clr-namespace:DAGlynEditor;assembly=DAGlynEditor"
        mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
        x:Class="DAGlynEditor.MainWindow"
        Title="DAGlynEditor">
    
    <Canvas x:Name="MainCanvas">
        <!--Canvas 수정해야 함.-->
        
        <controls:DAGlynEditor x:Name="EditorTester" Width="400" Height="400" 
                               x:DataType="controls:MainWindow" Canvas.Top="200" Canvas.Left="200" 
                               ItemsSource="{Binding ConItems}"/>
                               
    </Canvas>
   
